{
  "name": "chedda-sdk",
  "version": "1.0.1",
  "description": "SDK for interacting with the Chedda platform's smart contracts and services.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "require": "./index.cjs",
    "import": "./index.js"
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "start": "jest --watch",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --coverage --coverageReporters='text-summary'",
    "prepublishOnly": "tsc",
    "prepare": "tsc"
  },
  "keywords": [
    "chedda, ethereum"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "esbuild": "0.23.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "ethers": "^6.13.1"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  }
}
